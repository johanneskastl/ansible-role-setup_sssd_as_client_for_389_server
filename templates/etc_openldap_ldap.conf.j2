# See ldap.conf(5) for details
# This file should be world readable but not world writable.

BASE            {{ ldap_base }}
URI             {{ ldap_server_uri}}
{% if ldap_server_ca_certificate is defined %}
TLS_CACERT      {{ ldap_server_ca_certificate }}
{% endif %}

#SIZELIMIT      12
#TIMELIMIT      15
#DEREF          never
